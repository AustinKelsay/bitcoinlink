datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

model NWC {
  id            String   @id @default(cuid())
  url           String
  expiresAt     DateTime
  maxAmount     Int
  numLinks      Int
}